{
	"manifest_version": 2,
	"name": "V3rmifier",
	"version": "1.0.0",
	"default_locale": "en",
	"description": "An extension designed to notify the user about new threads on V3rmillion's lounge.",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.htm",
		"default_title": "V3rmifier"
	}
	"author": "cbpudding and SOLAREKUS",
	"content_scripts":[{
		"matches": [
			"http://*/*"
		],
		"js": [
			"jquery.3.2.1.min.js",
			"notify.js"
		]
	}],
	"permissions": [
		"storage",
		"notifications"
	]
}

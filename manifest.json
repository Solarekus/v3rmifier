{
	"name": "V3rmifier",
	"version": "1.0.0",
	"permissions": ["storage", "notifications"],
	"manifest_version": 2,
	"description": "V3rmillion Notifier Extension by cbpudding and SOLAREKUS from http://www.V3rmillion.net/",
	"content_scripts": [
			{
				"matches": ["http://v3rmillion.net/*"],
				"js": ["jquery.3.2.1.min.js", "notify.js"]
			}
	],
		
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "V3rmifier Notifications"
	}
  
}
{
	"name": "V3rmifier",
	"version": "1.0.0",
	"permissions": ["storage", "notifications"],
	"content_scripts": [ {
		"matches": ["http://*/*"],
		"js": ["notify.js","jquery.3.2.1.min.js"]
	} ],
	"manifest_version": 2,
	"description": "V3rmillion Notifier Extension by cbpudding and SOLAREKUS from http://www.V3rmillion.net/",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "V3rmifier Notifications"
	}
  
}